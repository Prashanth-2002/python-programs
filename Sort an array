arr=[]
n=int(input("Enter the number of elements in array"))
for i in range(0,n):
  l=int(input())
  arr.append(l)
print("unsorted array is"+ str(arr))
temp=0;

for i in range(0, len(arr)):    
 for j in range(i+1,len(arr)): 
   if(arr[i]>arr[j]):
    temp = arr[i];    
    arr[i] = arr[j];    
    arr[j] = temp;  
     
for i in range(0,len(arr)):
  print( (arr[i]))
