string=input("Enter a string ")
vowel_count={'a':0,'e':0,'i':0,'o':0,'u':0}\

for char in string:
  if char=='a':
    vowel_count['a']=vowel_count['a']+1
  elif char=='e':
    vowel_count['e']=vowel_count['e']+1
  elif char=='i':
    vowel_count['i']=vowel_count['i']+1
  elif char=='o':
    vowel_count['0']=vowel_count['0']+1
  elif char=='u':
    vowel_count['u']=vowel_count['u']+1


print(vowel_count)
